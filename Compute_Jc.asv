p(8) = e(1,1)*e(2,3)*e(3,2);
p(9) = e(1,1)*e(2,3)*e(3,3);

p(10) = e(1,2)*e(2,1)*e(3,1);
p(11) = e(1,2)*e(2,1)*e(3,2);
p(12) = e(1,2)*e(2,1)*e(3,3);

p(13) = e(1,2)*e(2,2)*e(3,1);
p(14) = e(1,2)*e(2,2)*e(3,2);
